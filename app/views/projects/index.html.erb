<div class='span12'>
	<h1>Portfolio</h1>
	<% @projects.each do |project| %>
		<h2><%= project.title %></h2>
		<%= image_tag project.image %>
		<p><%= project.when %></p>
		<p><%= project.website %></p>
		<p><%= project.github %></p>
		<p><%= project.description %></p>
		<p><%= project.tools %></p>
	<% end %>
</div>

